<p id="notice"><%= notice %></p>

<h1>Matches</h1>

<table>
  <thead>
    <tr>
      <th>Mentee_id</th>
      <th>Mentor_id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @matches_for_user.each do |key,match| %>
    <% if key == current_user %>
      <tr>
        <td><%= match.mentee_id %></td>
        <td><%= match.mentor_id %></td>
      </tr>
      <% end%>
    <% end %>
  </tbody>
</table>

<br>


