<header>
<nav class="navbar bg-transparent navbar-fixed-top py-5">
      <div class="container">
          <% if logged_in? %>
          <div class="col-sm-2">
          <%= link_to image_tag("logo1.png", size: "150x150", alt: "logo"), home_path, class: 'nav navbar-brand navbar-left', id: 'logo' %>
            </div>
            <ul class="nav navbar-nav navbar-right">
            <% @mentee = Mentee.find_by(user_id: current_user.id) %>
            <% @mentor = Mentor.find_by(user_id: current_user.id) %>
            <li><%= link_to "Chat", chat_path, method: :get, class: "r-link ai-element ai-element3" %></li> 
            <% if @mentee.nil? && @mentor.nil?%>
              <li><%= link_to "Log out", logout_path, method: :delete, class: "r-link ai-element ai-element3" %></li> 
            <% elsif current_user.mentee? %>
              <li><%= link_to "Explore", explore_path, method: :get, class: "r-link ai-element ai-element3"%></li> 
              <li><%= link_to "Profile", menteeprofile_path(@mentee), method: :get, class: "r-link ai-element ai-element3" %></li> 
              <li><%= link_to "Matches", matches_path, method: :get, class: "r-link ai-element ai-element3" %></li> 
              <li><%= link_to "Log out", logout_path, method: :delete, class: "r-link ai-element ai-element3" %></li> 
            <% elsif current_user.mentor? %>
              <li><%= link_to "Explore", explore_path, method: :get, class: "r-link ai-element ai-element3" %></li> 
              <li><%= link_to "Profile", mentorprofile_path(@mentor), method: :get, class: "r-link ai-element ai-element3" %></li> 
              <li><%= link_to "Matches", matches_path, method: :get, class: "r-link ai-element ai-element3" %></li> 
              <li><%= link_to "Log out", logout_path, method: :delete, class: "r-link ai-element ai-element3" %></li> 
            <% else %>
              <li><%= link_to "Explore", explore_path, method: :get, class: "r-link ai-element ai-element3" %></li> 
              <li><%= link_to "Profile", userprofile_path(current_user), method: :get, class: "r-link ai-element ai-element3" %></li> 
              <li><%= link_to "Matches", matches_path, method: :get, class: "r-link ai-element ai-element3" %></li> 
              <li><%= link_to "Profile", profile_path, method: :get, class: "r-link ai-element ai-element3" %></li> 
              <li><%= link_to "Log out", logout_path, method: :delete, class: "r-link ai-element ai-element3" %></li> 
              <% end %>
            </ul>
          <% end %>
        </div>
      </div>
   </nav>
</header> 
